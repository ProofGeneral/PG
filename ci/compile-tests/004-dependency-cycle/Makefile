.PHONY: test
test:
	$(MAKE) clean
	emacs -batch -l ../../../generic/proof-site.el -l ../cct-lib.el \
		-l runtest.el -f ert-run-tests-batch-and-exit

.PHONY: clean
clean:
	rm -f *.vo *.glob *.vio *.vos *.vok .*.aux
